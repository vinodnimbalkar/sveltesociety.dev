{
  "name": "svelte-society",
  "description": "Svelte Society Website",
  "version": "0.1.0",
  "scripts": {
    "dev": "run-p dev:routify dev:vite",
    "dev:routify": "routify run",
    "dev:vite": "vite",
    "build": "run-s build:routify build:vite",
    "build:routify": "routify run -b",
    "build:vite": "vite build"
  },
  "devDependencies": {
    "@sveltech/routify": "^1.9.0",
    "mdsvex": "^0.8.2",
    "npm-run-all": "^4.1.5",
    "routify-plugin-frontmatter": "^1.0.0",
    "svelte-hmr": "^0.10.0-0",
    "svite": "^0.2.1",
    "vite": "^1.0.0-beta.11"
  },
  "dependencies": {
    "prism-svelte": "^0.4.6",
    "prismjs": "^1.20.0",
    "svelte": "^3.23.2"
  },
  "routify": {
    "plugins": {
      "routify-plugin-frontmatter": {
        "extensions": [
          "md",
          "svx",
          "svelte"
        ]
      }
    }
  }
}